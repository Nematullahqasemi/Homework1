
package javaapplication47;


public class JavaApplication47 {

   
    public static void main(String[] args) {
    int x =32;
    System.out.println(x);
    x +=30;
        System.out.println(x);
    x -=25;
        System.out.println(x);
        x*=20;
        System.out.println(x);
        x/=15;
        System.out.println(x);
    }
    
}
